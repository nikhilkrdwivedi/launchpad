{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"]
      // Add more aliases here if needed
    }
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/**/*.js",
    "src/**/**/*.jsx"
  ]
}
