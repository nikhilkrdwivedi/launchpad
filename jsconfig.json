{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"],
      "@elements/*": ["src/elements/*"],
      "@constants/*": ["src/constants/*"],
      "@data/*": ["src/data/*"],
      "@hooks/*": ["src/hooks/*"],
      "@environment/*": ["src/environment/*"],
      "@utils/*": ["src/utils/*"],
      "@routes/*": ["src/routes/*"],
      "@helpers/*": ["src/helpers/*"]
      // Add more aliases here if needed
    }
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/**/*.js",
    "src/**/**/*.jsx",
    "src/**/**/**/*.js",
    "src/**/**/**/*.jsx"
  ]
}
